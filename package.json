{
    "name": "akw-api",
    "version": "1.0.0",
    "scripts": {
        "prepare": "husky",
        "start": "node dist/index.js",
        "dev": "nodemon",
        "build": "rimraf ./build &&  tsc && tsc-alias",
        "static:check": "eslint . && prettier --log-level silent --check --ignore-unknown .",
        "static:fix": "eslint . --fix && prettier --log-level silent --write --ignore-unknown .",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "circular:check": "npm run build && madge --circular dist",
        "circular:generate": "npm run build && madge --image circular.png dist",
        "db:add": "migrate-mongo create",
        "db:up": "migrate-mongo up",
        "db:down": "migrate-mongo down",
        "db:status": "migrate-mongo status"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "description": "",
    "dependencies": {
        "@typegoose/typegoose": "^12.10.1",
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.20.3",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.1",
        "class-validator-jsonschema": "^5.0.1",
        "compression": "^1.7.5",
        "cors": "^2.8.5",
        "dotenv": "^16.4.7",
        "express": "^4.21.2",
        "helmet": "^8.0.0",
        "http-status-codes": "^2.3.0",
        "husky": "^9.1.7",
        "jsonwebtoken": "^9.0.2",
        "migrate-mongo": "^11.0.0",
        "mongoose": "^8.9.4",
        "mongoose-paginate-v2": "^1.9.0",
        "morgan": "^1.10.0",
        "multer": "^1.4.3",
        "reflect-metadata": "^0.2.2",
        "rimraf": "^6.0.1",
        "routing-controllers": "^0.10.4",
        "routing-controllers-openapi": "^4.0.0",
        "swagger-jsdoc": "^6.2.8",
        "swagger-ui-express": "^5.0.1",
        "ts-node": "^10.9.2",
        "tsc-alias": "^1.8.10",
        "tsconfig-paths": "^4.2.0",
        "typedi": "^0.10.0",
        "typescript": "^5.7.3",
        "uuid": "^11.0.5"
    },
    "devDependencies": {
        "@types/bcryptjs": "^2.4.6",
        "@types/compression": "^1.7.5",
        "@types/express": "^5.0.0",
        "@types/jest": "^29.5.14",
        "@types/jsonwebtoken": "^9.0.7",
        "@types/morgan": "^1.9.9",
        "@types/multer": "^1.4.3",
        "@types/swagger-ui-express": "^4.1.7",
        "@typescript-eslint/eslint-plugin": "^8.19.1",
        "@typescript-eslint/parser": "^8.19.1",
        "eslint": "^9.17.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.2.1",
        "eslint-plugin-simple-import-sort": "^12.1.1",
        "jest": "^29.7.0",
        "madge": "^8.0.0",
        "nodemon": "^3.1.9",
        "prettier": "^3.4.2",
        "ts-jest": "^29.2.5"
    }
}
